#!/bin/sh

# shellcheck source=./setup
## TODO: What happens if $0 have no slashes?  Can that happen?
. "${0%/*}/setup"

# Test that EMACS_BIN is set to the value of EMACS if INSIDE_EMACS is
# not set

env -S -i - \
	EMACS="$fakemacs"            \
	PATH="${0%/*}:$defaultpath"  \
	BASH_ENV="$BASH_ENV" \
	BASH_XTRACEFD="$BASH_XTRACEFD" \
	KCOV_BASH_COMMAND="$KCOV_BASH_COMMAND" \
	KCOV_BASH_XTRACEFD="$KCOV_BASH_XTRACEFD" \
	PS4="$PS4" \
	"$buttercup" select emacs |
	checkline "$fakemacs -batch $defaultargs select emacs"
